    "dev-test": "set NODE_ENV=test & tsc & copyfiles --flat api-schema.yaml ./dist && copyfiles -E --flat ../locales/*.json ./dist/locales & node ./dist/server.js",
    "dev": "tsc & copyfiles --flat api-schema.yaml ./dist && copyfiles -E --flat ../locales/*.json ./dist/locales & node ./dist/server.js",
    "start": "tsc & copyfiles --flat api-schema.yaml ./dist && copyfiles -E --flat ../locales/*.json ./dist/locales & node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:watch": "npm run copy & tsc-w",
